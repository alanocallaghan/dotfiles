[user]
	email = alan.ocallaghan@outlook.com
	name = Alan O'Callaghan
	signingkey = /home/alan/.ssh/id_github.pub
[push]
	default = simple
[remind]
	paths = /home/alan/Documents/github/**,/home/alan/Documents/github/documents/*,/home/alan/Documents/github/carpentries/*,/home/alan/Documents/github/R/*,/home/alan/.config/sublime-text-3/Packages/User/,/home/alan/,/home/alan/Documents/github/wallace/*,/home/alan/Documents/github/imaging/*
[core]
	excludesfile = /home/alan/.gitignore_global
[credential]
	helper = cache
[url "ssh://git@github.com/"]
	insteadOf = https://github.com/
[url "ssh://git@gitlab.com/"]
	insteadOf = https://gitlab.com/
[init]
	defaultBranch = master
[pull]
	rebase = false
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[commit]
	gpgsign = true
[gpg]
	format = ssh
[rerere]
	enabled = true
